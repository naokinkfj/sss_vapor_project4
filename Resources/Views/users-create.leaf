#set("title") {Create account}

#set("body") {
    <h1>Create account</h1>
    <form method="post">
    <p>Username: <input type="text" name="username" /></p>
    #if(error) {
        <p>エラーがあるよ</p>
    }
    <p>Password: <input type="password" name="password" /></p>
    <p><input type="submit" class="btn btn-lg btn-primary" />
    </form>
}

#embed("master")